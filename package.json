{
  "name": "@libusoftcicom/lc-modal",
  "version": "1.0.0",
  "description": "LC Modal - Angular modal component.",
  "homepage": "https://github.com/LibusoftCicom/lc-modal",
  "bugs": "https://github.com/LibusoftCicom/lc-modal/issues",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "modal",
    "dialog",
    "ngx",
    "angular 14"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test lc-modal-lib ",
    "lint": "ng lint lc-modal-lib ",
    "e2e": "ng e2e",
    "copyReadme": "copy README.md .\\dist-lib\\ && copy LICENSE .\\dist-lib\\",
    "build:lib": "rimraf dist-lib && ng test lc-modal-lib && ng build lc-modal-lib --configuration production && npm run copyReadme",
    "build:app": "rimraf docs && ng build lc-modal-app --configuration production",
    "build:publish:local": "npm run build:lib && npm publish dist-lib/ --access public --registry http://localhost:4873",
    "build:publish:public": "npm run build:lib && npm publish dist-lib/ --access public --tag=latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "bootstrap": "4.3.1",
    "core-js": "2.4.1",
    "rxjs": "6.6.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "6.3.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "^15.2.2",
    "protractor": "7.0.0",
    "rimraf": "3.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.3",
    "typescript": "~4.8.4"
  },
  "main": "dist-lib/bundles/libusoftcicom-lc-modal.umd.js",
  "module": "dist-lib/fesm5/libusoftcicom-lc-modal.js",
  "es2015": "dist-lib/fesm2015/libusoftcicom-lc-modal.js",
  "esm5": "dist-lib/esm5/libusoftcicom-lc-modal.js",
  "esm2015": "dist-lib/esm2015/libusoftcicom-lc-modal.js",
  "fesm5": "dist-lib/fesm5/libusoftcicom-lc-modal.js",
  "fesm2015": "dist-lib/fesm2015/libusoftcicom-lc-modal.js",
  "typings": "dist-lib/libusoftcicom-lc-modal.d.ts",
  "metadata": "dist-lib/libusoftcicom-lc-modal.metadata.json"
}
