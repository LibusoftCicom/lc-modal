		"angular 4",
		"angular 5",
		"angular 6",
		"angular 7",
		"angular 8"
	],
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test lc-modal-lib ",
		"lint": "ng lint lc-modal-lib ",
		"e2e": "ng e2e",
